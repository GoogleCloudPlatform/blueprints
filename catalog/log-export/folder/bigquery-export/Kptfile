apiVersion: kpt.dev/v1
kind: Kptfile
metadata:
  name: bigquery-export
  annotations:
    blueprints.cloud.google.com/title: BigQuery Export blueprint
info:
  description: A log export on a folder that sinks to BigQuery
pipeline:
  mutators:
    - image: gcr.io/kpt-fn-demo/set-default-name:v0.5-yuwenma
      configPath: gcloud-config.yaml

    - image: gcr.io/kpt-fn-demo/set-project-id:v0.3-yuwenma
      configPath: gcloud-config.yaml

    - image: gcr.io/kpt-fn/set-namespace:v0.2.0
      configPath: gcloud-config.yaml

    - image: gcr.io/kpt-fn-demo/set-gcp-location:v0.3-yuwenma
      configPath: gcloud-config.yaml
